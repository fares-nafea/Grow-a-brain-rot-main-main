{
  "name": "Grow-a-brain-rot",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Remotes": {
        "$className": "Folder"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",

      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",

        "PromptClient": {
          "$path": "src/client/PromptClient.client.lua"
        }
      }
    },

    "StarterGui": {
      "$className": "StarterGui",

      "MainGui": {
        "$className": "ScreenGui",

        "Events": {
          "$className": "Folder",
          "ToggleSeedFrame": {
            "$className": "BindableEvent"
          }
        },

        "Root": {
          "$className": "Frame",

          "Frames": {
            "$className": "Frame",

            "SeedFrame": {
              "$className": "Frame",

              "FrameImage": {
                "$className": "ImageLabel"
              },
              "Restock": {
                "$className": "Frame",
                "RestockButton": {
                  "$className": "TextButton"
                }
              },
              "TitleFrame": {
                "$className": "Frame",
                "Title": {
                  "$className": "TextButton"
                }
              },
              "List": {
                "$className": "ScrollingFrame"
              }
            }
          },

          "GardenTP": {
            "$className": "Frame",
            "Button": { "$className": "TextButton" },
            "Image": { "$className": "ImageLabel" }
          },

          "SeedsTP": {
            "$className": "Frame",
            "Button": { "$className": "TextButton" },
            "Image": { "$className": "ImageLabel" }
          },

          "SellTP": {
            "$className": "Frame",
            "Button": { "$className": "TextButton" },
            "Image": { "$className": "ImageLabel" }
          },

          "Sheckles": {
            "$className": "TextLabel"
          }
        },

        "seedFrameHandler": {
          "$path": "src/client/seedFrameHandler.client.lua"
        },

        "buttonTPs": {
          "$path": "src/client/buttonTPs.client.lua"
        },

        "shecklesUpdater": {
          "$path": "src/client/shecklesUpdater.client.lua"
        },

        "Configuration": {
          "$className": "Folder",
          "seedFrameDebounce":{"$className": "BoolValue"},
          "seedFrameToggled":{"$className": "BoolValue"}
        }
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",

      "Server": {
        "$path": "src/server/Server.server.lua"
      },

      "Services": {
        "$className": "Folder",

        "DataService": {
          "$path": "src/server/services/DataService.lua",

          "ProfileStore": {
            "$path": "src/server/services/ProfileStore.lua"
          },

          "Template": {
            "$path": "src/server/services/Template.lua"
          }
        },

        "MoneyService": {
          "$path": "src/server/services/MoneyService.lua"
        },

        "PlotService": {
          "$path": "src/server/services/PlotService.lua"
        }
      }
    }
